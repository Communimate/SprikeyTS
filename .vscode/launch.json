  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Bot",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}\\dist\\bot.js",
      "skipFiles": [
        "<node_internals>/**"
      ],
      // "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    },
    {
      "name": "Build-Launch Bot",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}\\dist\\bot.js",
      "preLaunchTask": "pnpm: build",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}